# API Keys for language models
OPENAI_API_KEY=<>
GROQ_API_KEY=<>

# Agent configuration
DEFAULT_MODEL=gpt-3.5-turbo
DEFAULT_TEMPERATURE=0.7

# Server configuration
PORT=8000
HOST=0.0.0.0
LOG_LEVEL=INFO
ENVIRONMENT=development  # Change to "production" for production deployment

# Database configuration (for agent persistence)
DATABASE_URL=sqlite:///./Agenora.db

# Security
API_KEY_ENABLED=false  # Enable for production API key authentication
API_KEY=admin  # Set this in production
CORS_ORIGINS=http://localhost:8000,http://127.0.0.1:8000  # Add your production domain

# Performance
WORKER_TIMEOUT=300
MAX_WORKERS=4
KEEPALIVE=65
